.mobileWrapper{
  padding: 30rpx 30rpx 100rpx 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.case-item-bg{
  width: 335rpx;
  height: 682rpx;
  background-image: url('https://mini.0315678.cn/wxImg/case/phone-wrapper.png');
  background-size: 335rpx 682rpx;
  background-repeat: no-repeat;
}
.case-item-bg image{
  width: 292rpx;
  height: 518rpx;
  display: block;
  margin: 85rpx auto;
}
.case-item{
  display: flex;
  flex-direction: column;
}
.case-item .title{
  text-align: center;
  font-size: 22rpx;
  height: 82rpx;
  line-height: 82rpx;
  color: #1f1f1f;
}
